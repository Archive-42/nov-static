<pre class=3D"syntaxhighlighter-pre" data-syntaxhighlighter-params=3D"brush=
: java; gutter: false; theme: Confluence" data-theme=3D"Confluence">public =
class DijkstraShortestPath
{
    static class Vertex implements Comparable&lt;Vertex&gt;
    {
        String name;
        List&lt;Edge&gt; adjacencyList =3D new ArrayList&lt;&gt;();
        boolean visited;
        Vertex predecessor;
        int distance =3D Integer.MAX_VALUE;

        Vertex(String name)
        {
            this.name =3D name;
        }

        void addNeighbor(Edge edge)
        {
            adjacencyList.add(edge);
        }

        @Override
        public int compareTo(Vertex o)
        {
            return Integer.compare(distance, o.distance);
        }

        @Override
        public String toString()
        {
            return "Vertex{" +
                    "name=3D'" + name + '\'' +
                    '}';
        }
    }

    static class Edge
    {
        int weight;
        Vertex fromVertex;
        Vertex toVertex;

        Edge(int weight, Vertex fromVertex, Vertex toVertex)
        {
            this.weight =3D weight;
            this.fromVertex =3D fromVertex;
            this.toVertex =3D toVertex;

            fromVertex.addNeighbor(this);
        }
    }

    static private void computePathFrom(Vertex sourceVertex)
    {
        sourceVertex.distance =3D 0;

        PriorityQueue&lt;Vertex&gt; pq =3D new PriorityQueue&lt;&gt;();
        pq.add(sourceVertex);
        while(!pq.isEmpty())
        {
            Vertex vertex =3D pq.poll();
            if(!vertex.visited)
            {
                vertex.visited =3D true;
                for(Edge edge : vertex.adjacencyList)
                {
                    Vertex targetVertex =3D edge.toVertex;
                    int distance =3D vertex.distance + edge.weight;
                    if(targetVertex.distance &gt; distance)
                    {
                        targetVertex.distance =3D distance;
                        targetVertex.predecessor =3D vertex;
                        pq.add(targetVertex);
                    }
                }
            }
        }
    }

    private static int getShortestPath(Vertex targetVertex, List&lt;Vertex&=
gt; result)
    {
        for(Vertex vertex =3D targetVertex; vertex !=3D null; vertex =3D ve=
rtex.predecessor)
        {
            result.add(vertex);
        }
        int distance =3D result.get(0).distance;
        Collections.reverse(result);
        return distance;
    }

    static int getShortestPath(Vertex fromVertex, Vertex toVertex, List&lt;=
Vertex&gt; path)
    {
        computePathFrom(fromVertex);
        return getShortestPath(toVertex, path);
    }
}</pre>=20
