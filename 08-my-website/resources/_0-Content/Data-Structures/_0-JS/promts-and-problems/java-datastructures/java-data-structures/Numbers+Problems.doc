class GasStations
{
//    Input is provided in a 2D array, for example:
//    {4, 6},
//    {6, 5},
//    {1, 3},
//    {7, 4},
//    Where the first value is the amount of petrol and the next value is t=
he distance to the next station.
//    To make it easier, we convert each coordinate to a Unit {petrol, dist=
ance}

    private static class Unit
    {
        int petrol;
        int distance;
        Unit(int petrol, int distance)
        {
            this.petrol =3D petrol;
            this.distance =3D distance;
        }
    }

    static int startingStation(int[][] data)
    {
        List&lt;Unit&gt; units =3D Arrays.stream(data).map((int[] a) -&gt; =
new Unit(a[0], a[1])).collect(Collectors.toList());

        int supply =3D 0;

        for(int i =3D 0; i &lt; units.size(); ++i)
        {
            Unit unit =3D units.get(i);
            if(unit.distance &gt; unit.petrol + supply)
            {
                continue;
            }

            //  Check if we can visit all stations from "good" starting poi=
nt
            int j =3D 0;
            for(; j &lt; units.size(); ++j)
            {
                Unit currentUnit =3D units.get((i + j) % units.size());
                if(currentUnit.distance &gt; currentUnit.petrol + supply)
                {
                    break;
                }
                supply +=3D currentUnit.petrol - currentUnit.distance;
            }

            //  if we could visit all stations
            if(j =3D=3D units.size())
            {
                return i;
            }
            //  Try competing tour from the next station
            supply =3D 0;
        }
        return -1;
    }
}</pre>=20
